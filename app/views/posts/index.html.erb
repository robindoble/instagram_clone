<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<% if @posts.any? %>
	<% @posts.each do |post| %>
	<%= post.caption %>
	<% end %>
<% else %>
	<p> No posts yet</p>
<% end %>

